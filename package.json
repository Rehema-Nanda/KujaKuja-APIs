{
  "name": "kujakuja-3-api",
  "description": "KujaKuja API",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "author": "KujaKuja",
  "repository": {
    "type": "git",
    "url": "https://github.com/kujakuja/3.0_api.git"
  },
  "scripts": {
    "prepare-db": "knex migrate:latest --knexfile ./src/config/db.js && knex seed:run --knexfile ./src/config/db.js",
    "knex:migrate:make": "knex --knexfile ./src/config/db.js migrate:make",
    "deploy": "gcloud app deploy",
    "start": "node ./src/server.js",
    "debug": "nodemon --inspect ./src/server.js",
    "test": "jest --runInBand",
    "lint": "eslint .",
    "preinstall": "([ ! -f package-lock.json ] && npm install --package-lock-only --ignore-scripts --no-audit); npx npm-force-resolutions",
    "kubernetes-create-cluster": "node kubernetes/create_cluster.js",
    "kubernetes-create-deployment": "kubectl apply -f kubernetes/manifests/deployment_manifest.yaml",
    "kubernetes-expose-deployment": "kubectl apply -f kubernetes/manifests/service_manifest.yaml"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^4.7.0",
    "@google-cloud/language": "^3.5.0",
    "@google-cloud/logging-bunyan": "^3.0.1",
    "@google-cloud/pubsub": "^1.1.2",
    "@google-cloud/storage": "^4.3.1",
    "@google-cloud/tasks": "^1.5.0",
    "@google-cloud/translate": "^5.3.0",
    "@sendgrid/mail": "^6.3.1",
    "@slack/web-api": "^5.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "debug": "^4.1.1",
    "engine.io": "^6.1.3",
    "express": "^4.16.3",
    "googleapis": "^47.0.0",
    "http-status": "^1.3.2",
    "json-bigint": "1.0.0",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^8.3.0",
    "knex": "^0.20.13",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "multer": "^1.4.1",
    "node-forge": "^1.3.0",
    "node-notifier": "^8.0.1",
    "npm-force-resolutions": "0.0.10",
    "object-hash": "^1.3.0",
    "objection": "^2.2.16",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.1",
    "socket.io": "^2.4.1",
    "uuid": "^3.3.2"
  },
  "resolutions": {
    "node-forge": "1.3.0",
    "json-bigint": "1.0.0",
    "engine.io": "6.1.3"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^25.5.4",
    "knex-db-manager": "github:kujakuja/knex-db-manager#fixes",
    "nodemon": "^2.0.4",
    "npm-force-resolutions": "0.0.10",
    "pg-escape": "^0.2.0",
    "shelljs": "^0.8.5",
    "supertest": "^3.1.0"
  }
}
